---
// Importar los componentes necesarios
import Contact from '../components/Contact/Contact.astro';
import Hero from '../components/Hero/Hero.astro';
import Process from '../components/Process/Process.jsx';
import Projects from '../components/Projects/Projects.astro';
import Layout from '../layouts/Layout.astro';
import Loader from '../components/UI/Loader/Loader.jsx';
import WhatsAppButton from '../components/UI/Wp-Button/WpButton';
---

<Layout title="Alecarp | Muebles a medida">
  <main>
      <Loader client:visible/>
      <Hero />
      <Projects />
      <Process client:visible />
      <Contact />
      <WhatsAppButton
        phoneNumber="5491161045505"
        message="Hola, estoy interesado en muebles a medida y quisiera más información sobre su proceso y diseños."
        client:load
      />
  </main>
</Layout>

